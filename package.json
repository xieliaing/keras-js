{
  "name": "keras-js",
  "version": "1.0.2",
  "description": "Run Keras models in the browser, with GPU support using WebGL",
  "main": "lib/index.js",
  "unpkg": "dist/keras.min.js",
  "scripts": {
    "watch": "NODE_ENV=development BABEL_ENV=browser webpack --watch --config webpack.config.js",
    "build:browser": "IF exist dist (rmdir /S /Q dist)&& SET NODE_ENV=production && SET BABEL_ENV=browser && webpack --config webpack.config.js",
    "build:node": "IF exist lib (rmdir /S /Q lib)&& SET NODE_ENV=production && SET BABEL_ENV=node && babel src --out-dir lib",
    "build": "npm run build:browser && npm run build:node",
    "server": "http-server . -c-1 -p 3000",
    "demos:watch": "cd demos; SET NODE_ENV=development BABEL_ENV=browser webpack --watch --config webpack.config.js",
    "demos:build": "cd demos; IF exist dist (rm /S /Q dist); SET NODE_ENV=production BABEL_ENV=browser webpack --config webpack.config.js",
    "proto2js": "pbjs --target static-module --wrap es6 proto/model.proto > src/proto.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transcranial/keras-js.git"
  },
  "keywords": ["keras", "deep", "learning", "neural", "networks", "javascript", "webgl", "webgl2", "gpu"],
  "author": "Leon Chen <leon@md.ai>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/transcranial/keras-js/issues"
  },
  "homepage": "https://github.com/transcranial/keras-js#readme",
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.36",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "cwise": "^1.0.10",
    "eventemitter3": "^3.0.0",
    "lodash": "^4.17.4",
    "ndarray": "^1.0.18",
    "ndarray-blas-level2": "^1.1.0",
    "ndarray-concat-rows": "^1.0.1",
    "ndarray-gemm": "^1.0.0",
    "ndarray-ops": "^1.2.2",
    "ndarray-resample": "^1.0.1",
    "ndarray-squeeze": "^1.0.2",
    "ndarray-tile": "^1.0.3",
    "ndarray-unpack": "^1.0.0",
    "ndarray-unsqueeze": "^1.0.3",
    "performance-now": "^2.1.0",
    "protobufjs": "^6.8.3"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.36",
    "@babel/core": "7.0.0-beta.36",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.36",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.36",
    "@babel/preset-env": "7.0.0-beta.36",
    "babel-eslint": "^8.1.2",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-lodash": "^3.3.2",
    "blueimp-load-image": "^2.17.1",
    "chroma-js": "^1.3.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "http-server": "^0.10.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "style-loader": "^0.19.1",
    "vue": "^2.5.13",
    "vue-loader": "^13.6.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vuetify": "^0.17.6",
    "webpack": "^3.10.0"
  }
}
